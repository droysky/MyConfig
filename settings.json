{
    "window.zoomLevel": 1,
    "editor.minimap.enabled": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "none",
    "editor.fontSize": 16,
    "workbench.activityBar.visible": false,
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.3,
    "editor.cursorBlinking": "solid",
    "editor.detectIndentation": false,
    "editor.guides.indentation": false,
    "editor.cursorSurroundingLines": 10,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 6,
    "editor.scrollbar.horizontalScrollbarSize": 5,
    "editor.renderLineHighlight": "none",
    "editor.matchBrackets": "near",
    // время автодополнения
    "editor.quickSuggestionsDelay": 1000,
    "workbench.tips.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "settingsSync.ignoredSettings": [],
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.lineNumbers": "relative",
    "settingsSync.ignoredExtensions": [],
    // отбражать фукцию в ввержней колонке
    "breadcrumbs.symbolPath": "off",
    // цвета
    // https://code.visualstudio.com/api/references/theme-color
    "workbench.colorCustomizations": {
        "editor.background": "#0E1C1E",
        "sideBar.background": "#0E1C1E",
        "activityBar.background": "#0E1C1E",
        "focusBorder": "#095a66",
        //"scrollbarSlider.background": "#095a66",
        "tab.inactiveBackground": "#0E1C1E",
        "editorGroupHeader.tabsBackground": "#0E1C1E",
        "editorCursor.background": "#e9e7e7",
        "editorCursor.foreground": "#095a66",
        "terminalCursor.background": "#e9e7e7",
        "terminalCursor.foreground": "#095a66",
    },
    "C_Cpp.clang_format_fallbackStyle": "google",
    "C_Cpp.clang_format_style": "google",
    "editor.formatOnSave": true,
    "clang.cxxflags": [
        "-std=c++20",
        "-Wall",
        "-Wextra",
        "-Werror",
        "-Waddress",
        "-Wpedantic",
    ],
    "files.autoSave": "onFocusChange",
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/man/.config/nvim/init.vim",
    "cmake.configureOnOpen": false,
    "cmake.configureSettings": {},
    "editor.occurrencesHighlight": false,
    "editor.lightbulb.enabled": false,
    "extensions.autoCheckUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "extensions.autoUpdate": false,
    "editor.fontVariations": false,
    "editor.scrollbar.scrollByPage": true,
    "audioCues.diffLineDeleted": "off",
    "audioCues.diffLineInserted": "off",
    "audioCues.diffLineModified": "off",
    "audioCues.lineHasBreakpoint": "off",
    "audioCues.lineHasError": "off",
    "audioCues.lineHasFoldedArea": "off",
    "audioCues.lineHasInlineSuggestion": "off",
    "audioCues.noInlayHints": "off",
    "audioCues.notebookCellCompleted": "off",
    "audioCues.notebookCellFailed": "off",
    "audioCues.onDebugBreak": "off",
    "audioCues.taskCompleted": "off",
    "audioCues.taskFailed": "off",
    "audioCues.terminalCommandFailed": "off",
    "audioCues.terminalQuickFix": "off",
    "audioCues.volume": 10,
    "editor.accessibilitySupport": "off",
    "workbench.statusBar.visible": false,
    "terminal.integrated.enableMultiLinePasteWarning": false
}
